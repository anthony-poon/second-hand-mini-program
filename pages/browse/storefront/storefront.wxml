<view class="page">
    <view class="page__hd">
        <view class="page__title">{{sfName}}</view>
    </view>
    <view class="page__bd">
        <view class="simple-container" wx:for="{{products}}" wx:for-item="item" wx:key="id">
          <template is="{{ item.template }}" data="{{ item: item }}"/>
        </view>
    </view>
    <import src="/tabbar.wxml"/>
    <template is="tabBar" data="{{tabBar}}"/>
</view>


