<template name="second-hand">
  <view class="weui-form-preview">
      <view wx:if="{{item.img.length > 0}}" class="swiper-container">
          <swiper class="swiper_box" bindchange="swiperchange">
              <block wx:for="{{item.img}}" wx:key="id">
                  <swiper-item>
                      <image src="{{item}}" mode="aspectFit"></image>
                  </swiper-item>
              </block>
          </swiper>
          <view class="dots">
              <block wx:for="{{item.img}}" wx:key="unique">
                  <view wx:if="{{item.img.length > 1}}"
                        class="dot{{index == swiperCurrent ? ' active' : ''}}"></view>
              </block>
          </view>
      </view>
      <view class="weui-form-preview__hd">
          <view class="weui-form-preview__item">
              <view class="weui-form-preview__label">商品</view>
              <view class="weui-form-preview__value_in-hd">{{ item.title }}</view>
          </view>
      </view>
      <view class="weui-form-preview__bd">

          <view class="weui-form-preview__item">
              <view class="weui-form-preview__label">描述</view>
              <view class="weui-form-preview__value">{{ item.desc }}</view>
          </view>
          <view class="weui-form-preview__item">
              <view class="weui-form-preview__label">金额</view>
              <view class="weui-form-preview__value">¥{{ item.price }}</view>
          </view>
          <view class="weui-form-preview__item">
              <view class="weui-form-preview__label">浏览量</view>
              <view class="weui-form-preview__value">{{ item.visitorCount }}</view>
          </view>
      </view>
  </view>
</template>

